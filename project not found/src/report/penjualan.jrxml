	<queryString>
		<![CDATA[SELECT 
    p.no_penjualan,
    p.tgl_jual,
    p.total_harga AS total_harga_penjualan,
    p.bayar,
    p.kembali,
    p.jenis_bayar,
    p.id_pengguna,
    dp.no_penjualan AS no_detail_penjualan,
    dp.kode_barang AS kode_barang_detail,
    dp.jumlah,
    dp.diskon,
    dp.total_harga AS total_harga_detail,
    b.kode_barang,
    b.nama_barang,
    b.harga_jual,
    b.no_barcode
FROM 
    penjualan p
JOIN 
    detail_penjualan dp ON p.no_penjualan = dp.no_penjualan
JOIN 
    barang b ON dp.kode_barang = b.kode_barang;]]>
	</queryString>
